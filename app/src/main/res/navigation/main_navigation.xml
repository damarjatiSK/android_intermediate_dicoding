<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.damar.submitinter2.home.HomeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_home"/>

    <fragment
        android:id="@+id/navigation_foto"
        android:name="com.damar.submitinter2.addstory.AddStoryFragment"
        android:label="Foto"
        tools:layout="@layout/fragment_add_story" />

    <fragment
        android:id="@+id/navigation_map"
        android:name="com.damar.submitinter2.map.MapFragment"
        android:label="Map Story"
        tools:layout="@layout/fragment_map" />

    <fragment
        android:id="@+id/navigation_profil"
        android:name="com.damar.submitinter2.profil.ProfilFragment"
        android:label="Profile"
        tools:layout="@layout/fragment_profil"/>
</navigation>